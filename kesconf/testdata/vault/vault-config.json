{
  "api_addr": "https://127.0.0.1:8200",
  "backend": {
    "file": {
      "path": "/tmp/vault/file"
    }
  },

  "default_lease_ttl": "168h",
  "max_lease_ttl": "720h",
  "disable_mlock": true,

  "listener": {
    "tcp": {
      "address": "0.0.0.0:8200",
      "tls_cert_file": "vault.crt",
      "tls_key_file": "vault.key",
      "tls_min_version": "tls12"
    }
  }
}

